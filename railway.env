# Railway Environment Template - DO NOT PUT REAL SECRETS HERE
# Set actual values in Railway dashboard: https://railway.app/project/your-project-id

# Core Application Settings
APP_ENV=production
NODE_ENV=production
PORT=3000
PYTHONPATH=/app
PYTHONUNBUFFERED=1

# Webhook Configuration - IMPORTANT FOR YOUR NEW WEBHOOK SYSTEM
WEBHOOK_BASE_URL=https://your-railway-app.up.railway.app
WEBHOOK_CREATOMATE_URL=https://your-railway-app.up.railway.app/api/webhooks/creatomate

# Queue Processing Configuration
ENABLE_CONCURRENT_PROCESSING=false
MAX_WORKERS=1

# Database (set real values in Railway dashboard)
SUPABASE_URL=placeholder
SUPABASE_KEY=placeholder

# AI Services (set real values in Railway dashboard)  
OPENAI_API_KEY=placeholder
HEYGEN_API_KEY=placeholder
VIZARD_API_KEY=placeholder

# Video Processing (set real values in Railway dashboard)
CREATOMATE_API_KEY=placeholder
CLOUDINARY_CLOUD_NAME=placeholder
CLOUDINARY_API_KEY=placeholder
CLOUDINARY_API_SECRET=placeholder

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# Optional - External Webhook Notifications
# WEBHOOK_URLS=https://hooks.slack.com/your-webhook
# WEBHOOK_EVENTS=job.completed,job.failed
# WEBHOOK_SECRET_KEY=your-secret